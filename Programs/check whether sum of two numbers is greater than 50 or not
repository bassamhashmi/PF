#include <iostream>
using namespace std;

int main() {
	int a, b, sum;
	
	cout << "Enter first number: ";
	cin >> a;
	cout << "Enter second number: ";
	cin >> b;
	
	sum = a + b;
		
	cout << "The sum of two numbers is = " << sum << endl;
	
	if(sum < 50) {
		cout << "Their sum is lesser than 50" << endl;
	} else if (sum > 50) {
		cout << "Their sum is greater than 50" << endl;
	} else if (sum == 50) {
		cout << "Their sum is equal to 50" << endl;
	}
	
	return 0;
}
